{
  "sweetpad.build.xcodeWorkspacePath": "Hohma.xcodeproj/project.xcworkspace",
  "sweetpad.build.cleanBuild": false,
  "sweetpad.build.buildConfiguration": "Debug",
  "editor.formatOnSave": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/DerivedData/**": false
  },
  "swift.path": "/usr/bin/swift",
  "swift.sourcekit-lsp.serverPath": "/usr/bin/sourcekit-lsp",
  "outline.showFunctions": true,
  "outline.showVariables": true,
  "outline.showClasses": true,
  "outline.showStructs": true,
  "outline.showInterfaces": true,
  "outline.showEnums": true,
  "outline.showModules": true,
  "outline.showNamespaces": true,
  "outline.showPackages": true,
  "outline.showFields": true,
  "outline.showMethods": true,
  "outline.showProperties": true,
  "outline.showEvents": true,
  "outline.showOperators": true,
  "outline.showTypeParameters": true,
  "outline.showConstants": true,
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "breadcrumbs.symbolPath": "on",
  "breadcrumbs.symbolSortOrder": "position",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.suggest.showSnippets": true,
  "editor.suggest.localityBonus": true,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.minimap.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.minimap.renderCharacters": true,
  "editor.minimap.maxColumn": 300,
  "editor.minimap.side": "right",
  "editor.minimap.size": "proportional",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.highlightActiveIndentation": true,
  "editor.guides.indentation": true,
  "editor.stickyScroll.enabled": true,
  "editor.stickyScroll.maxLines": 5,
  "workbench.editor.enablePreview": false,
  "workbench.editor.revealIfOpen": true,
  "better-comments.tags": [
    {
      "tag": "MARK",
      "color": "#007acc",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": true,
      "italic": false
    },
    {
      "tag": "TODO",
      "color": "#ff8c00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "FIXME",
      "color": "#f44747",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "NOTE",
      "color": "#4ec9b0",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ]
}
